{
  "name": "vscode-plugin-test",
	"displayName": "vscode-plugin-test",
	"description": "vscode 插件示例",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:vscode-plugin-test.helloWorld",
				"onLanguage:json"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [{
            "command": "vscode-plugin-test.helloWorld",
            "title": "Hello World"

		}],
		"keybindings": [
			{
				"command": "vscode-plugin-test.helloWorld",
				"key": "ctr+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorTextFocus",
					"command": "vscode-plugin-test.helloWorld",
					"group": "navigation"
				}
			],
			"editor/title": [],
			"editor/title/context": [],
			"explorer/context": []
		},
		"snippets": [
			{
					"language": "javascript",
					"path": "./snippets/javascript.json"
			}
		],
		"viewsContainers": {}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    }

}
